{
  "name": "valorant-scrambler",
  "description": "Team scrambler for Valorant. Teams randomizer, map and agents selected as random.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "gh-pages -d dist",
